}
@article{Whitefoot2010,
abstract = {In order to reduce greenhouse gas emissions and oil consumption, Congress recently amended the Corporate Average Fuel Economy regulation to encourage improvements in the fuel efficiency of new vehicles. Automotive firms have multiple strategies available to them to respond to this policy intervention, and the responses they choose have potentially significant welfare implications. This paper presents one of the first analyses of the reformed CAFE, explicitly accounting for medium-term compliance strategies that include both technology implementation and tradeoffs between vehicle attributes. We use physics-based simulations to identify engineering tradeoffs between fuel economy and other vehicle attributes independent of unobservable vehicle attributes. Final estimation of the demand side is still pending, but we present preliminary results using a demonstrational demand-side estimation. Preliminary results indicate that while firms rely on technology implementation to a significant extent to comply with the reformed CAFE, only 50{\%} of the potential reductions in fuel consumption from these technologies are realized, with the remainder applied to improving acceleration performance. Results also suggest that ignoring tradeoffs between vehicle attributes significantly overestimates the social surplus cost of the regulation. The methods presented are generalizable to many other consumer product industries with energy-efficiency regulations or incentives.},
author = {Whitefoot, Katie and Fowlie, Meredith and Skerlos, Steven},
