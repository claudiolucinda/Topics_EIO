number = {1710},
title = {{Discrete Choice Models as Structural Models of Demand: Some Economic Implications of Common Approaches}},
url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.79.2914{\&}rep=rep1{\&}type=pdf},
year = {2001}
}
