@article{Erdem1996,
abstract = {Internal market structure analysis infers brand positions in an$\backslash$nattribute space from preference and choice data, given a market in which$\backslash$nconsumers have heterogeneous tastes for attributes. Previous market$\backslash$nstructure models have adopted a static framework (e.g., Elrod 1988,$\backslash$nChintagunta 1994, Elrod and Keane 1995). Furthermore, they assumed that$\backslash$nconsumer perceptions of brand attributes do not vary across consumers.$\backslash$nYet, these approaches may render inaccurate representations of market$\backslash$nstructure if there is state dependence in consumer choice behavior. This$\backslash$npaper attempts to incorporate consumer choice dynamics into market$\backslash$nstructure models by specifying the source of choice dynamics explicitly.$\backslash$nIn particular, the process by which past purchases affect current$\backslash$nchoices is modeled in a framework which captures both consumer habit$\backslash$npersistence and variety seeking behavior. More specifically, consumer$\backslash$npreferences for brand attributes are modeled to depend on the attributes$\backslash$nof brands bought on the previous purchase occasion. Furthermore, the$\backslash$nmodeling approach adopted incorporates heterogeneity in both consumer$\backslash$npreferences and perceptions of brand attributes. The audience of this$\backslash$npaper includes practitioners and academics interested in understanding$\backslash$nconsumer choice processes and inferring market structure from consumer$\backslash$nchoice data.$\backslash$nThe proposed models are estimated on Nielsen scanner panel data for$\backslash$nmargarine, peanut butter, yogurt, and liquid detergent using simulated$\backslash$nmaximum likelihood techniques. The empirical results suggest that$\backslash$naccounting for choice dynamics improves both in-sample and out-of-sample$\backslash$nfit. The results indicate that the average consumer is habit persistent$\backslash$nin all the product categories studied. This result is consistent with$\backslash$nthe findings of Kannan and Sanchez (1994), who conducted an aggregate$\backslash$nanalysis of consumer variety seeking behavior across product categories.$\backslash$nHowever, the results obtained in this paper suggest that consumers are$\backslash$nheterogeneous with respect to the processes by which past purchases$\backslash$naffect current purchases.$\backslash$nThese results provide strong evidence for habit persistence and variety$\backslash$nseeking in brand attributes to be the behavioral source of consumer$\backslash$nchoice dynamics in food categories. Thus, consumer tastes (utility$\backslash$nweights) seem to be affected by the attributes of the brands consumed in$\backslash$nthe past. Given the empirical result that a large proportion of$\backslash$nconsumers are habit persistent, this suggests that tastes are reinforced$\backslash$nby the brand attributes consumed in the past.$\backslash$nThe empirical results also show that not accounting for state dependence$\backslash$nin market structure models for panel data may produce misleading$\backslash$nresults, that is, depending on consumer behavior patterns, models that$\backslash$ndo not account for state dependence may distort the true nature of$\backslash$ncompetition among brands. More specifically, the results confirm the$\backslash$nexpectation that if there is habit persistence, that is, if consumer$\backslash$ntastes are reinforced by attributes of brands consumed in the past,$\backslash$nmodels that do not capture this choice dynamics will overestimate the$\backslash$ndistance between (similar) brands.$\backslash$nFurthermore, the policy experiments conducted suggest that (1) static$\backslash$nmodels overestimate the short-run impact of a price cut on the sales of$\backslash$nthe brand on promotion, (2) price cuts hurt the sales of the more$\backslash$nsimilar brands more, and (3) free samples affect relatively less similar$\backslash$nbrands the most.$\backslash$nFinally, this paper studies variety seeking and habit persistence across$\backslash$nbrands over purchase occasions. However, variety seeking behavior may$\backslash$nalso involve the purchase of a portfolio of brands or items at a$\backslash$npurchase occasion. Consumers may buy multiple items knowing that prior$\backslash$nto the next trip they may want to consume different items (Simanson$\backslash$n1990, Walsh 1995). This type of behavior can be modeled within the$\backslash$ncontext of dynamic expected utility maximization with forward-looking$\backslash$nconsumers. The development and estimation of market structure models$\backslash$nthat include forward-looking consumers who maximize expected-utility$\backslash$nover a planning horizon, incorporating their future tastes and needs,$\backslash$nand shopping for an inventory of brands, remain an important future$\backslash$nresearch issue.},
author = {Erdem, T},
doi = {10.1287/mksc.15.4.359},
file = {:C$\backslash$:/Users/ClaudioLucinda/Dropbox/Lucinda-Moita/Bibliografia/Erdem1996.pdf:pdf},
isbn = {0732-2399},
